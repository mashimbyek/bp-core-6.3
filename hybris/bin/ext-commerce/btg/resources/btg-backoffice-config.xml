                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="usedRuleTypes"/>
                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="operandMapping"/>
                </editorArea:section>
            </editorArea:tab>
        </editorArea:editorArea>
    </context>
