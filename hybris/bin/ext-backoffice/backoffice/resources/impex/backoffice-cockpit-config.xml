<config xmlns="http://www.hybris.com/cockpit/config">
	<context type="com.hybris.cockpitng.util.Range" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels beanId="rangeLabelProvider"/>
        </y:base>
    </context>

	<context component="simple-list" type="java.lang.String">
		<ysl:simple-list xmlns:ysl="http://www.hybris.com/cockpitng/config/simplelist">
			<ysl:name field="#root"/>
			<ysl:description field="class.canonicalName"/>
		</ysl:simple-list>
	</context>

    <context type="java.lang.Object" component="listview">
        <list-view:list-view xmlns:list-view="http://www.hybris.com/cockpitng/component/listView">
            <list-view:column label="hmc.value" qualifier="toString()"/>
        </list-view:list-view>
    </context>

	<context type="java.util.Collection" component="base">
		<y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
			<y:labels beanId="collectionLabelProvider"/>
		</y:base>
    </context>
    <context type="java.util.Map" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels beanId="mapLabelProvider"/>
        </y:base>
    </context>
	<context type="java.util.Locale" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels beanId="localeLabelProvider"/>
        </y:base>
    </context>
	<context type="java.util.Date" component="base">
		<y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
			<y:labels beanId="dateLabelProvider"/>
		</y:base>
	</context>

</config>
